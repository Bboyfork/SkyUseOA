<view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="member"><view class="header"><view class="title">成员</view><view data-event-opts="{{[['tap',[['addMember',['$event']]]]]}}" class="addbtn" bindtap="__e"><image src="../../static/image/add.png" mode="widthFix"></image><view>添加</view></view></view><view class="memberList"><uni-collapse vue-id="11fc2b2c-1" accordion="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{memberList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><uni-collapse-item vue-id="{{('11fc2b2c-2-'+index)+','+('11fc2b2c-1')}}" title="{{'成员'+(index+1)}}" show-animation="{{item.animation}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="memberListItem"><view class="uni-form-item uni-column"><view class="title">姓名<text class="red">*</text></view><input class="uni-input" placeholder="请输入成员姓名" data-event-opts="{{[['input',[['__set_model',['$0','memberName','$event',[]],[[['memberList','id',item.id]]]]]]]}}" value="{{item.memberName}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">与本人关系<text class="red">*</text></view><input class="uni-input" placeholder="请输入与本人关系" data-event-opts="{{[['input',[['__set_model',['$0','memberRelation','$event',[]],[[['memberList','id',item.id]]]]]]]}}" value="{{item.memberRelation}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">籍贯<text class="red">*</text></view><input class="uni-input" placeholder="请输入籍贯" data-event-opts="{{[['input',[['__set_model',['$0','memberNativePlace','$event',[]],[[['memberList','id',item.id]]]]]]]}}" value="{{item.memberNativePlace}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">出生年月<text class="red">*</text></view><picker mode="date" value="{{item.memberBrithday}}" name="memberBrithday" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['changeDate',['$event','$0'],[[['memberList','id',item.id]]]]]]]}}" bindchange="__e"><view class="uni-input">{{item.memberBrithday}}</view></picker></view><view class="uni-form-item uni-column"><view class="title">单位</view><input class="uni-input" placeholder="请输入单位" data-event-opts="{{[['input',[['__set_model',['$0','memberWorkUnit','$event',[]],[[['memberList','id',item.id]]]]]]]}}" value="{{item.memberWorkUnit}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">职务</view><input class="uni-input" placeholder="请输入职务" data-event-opts="{{[['input',[['__set_model',['$0','memberJob','$event',[]],[[['memberList','id',item.id]]]]]]]}}" value="{{item.memberJob}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">电话<text class="red">*</text></view><input class="uni-input" placeholder="请输入电话号码" data-event-opts="{{[['input',[['__set_model',['$0','memberPhone','$event',[]],[[['memberList','id',item.id]]]]]]]}}" value="{{item.memberPhone}}" bindinput="__e"/></view></view></uni-collapse-item></block></uni-collapse></view></view><block wx:if="{{memberList.length!==0}}"><view class="button-sp-area"><button class="saveBtn" type="primary" data-event-opts="{{[['tap',[['saveRelation',['$event']]]]]}}" bindtap="__e">保存</button></view></block></form></view>